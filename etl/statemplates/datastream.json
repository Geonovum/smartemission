{{
  "name": "{name}",
  "description": "{label} for Station {station_id}",
  "unitOfMeasurement": {{
    "name": "{label}",
    "symbol": "{unit}",
    "definition": "http://unitsofmeasure.org/ucum.html#para-30"
  }},
  "observationType": "http://www.opengis.net/def/observationType/OGC-OM/2.0/OM_Measurement",

  "Thing": {{
    "@iot.id": {thing_id}
  }},
  "ObservedProperty": {{
    "name": "{name}",
    "definition": "http://sweet.jpl.nasa.gov/ontology/property.owl",
    "description": "{label}"
  }},
  "Sensor": {{
    "name": "{name}",
    "description": "{label} sensor for Station {station_id}",
    "encodingType": "python",
    "metadata": "https://github.com/Geonovum/smartemission/blob/master/etl/sensordefs.py"
  }}
}}
